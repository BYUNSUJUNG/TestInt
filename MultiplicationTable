import java.util.*;public class MultiplicationTable  {
 public static void main(String[] args) {  int num1, num2=1;  Scanner scan = new Scanner(System.in);    System.out.print("구구단 중에서 출력하고 싶은 단을 입력하시오: ");  num1=scan.nextInt();  while(num2<10) {   System.out.println(num1+"*"+num2+"="+(num1*num2));   num2++;  } }}
