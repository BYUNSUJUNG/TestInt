import java.util.Scanner;public class Sum2 {
 public static void main(String[] args) {  Scanner input= new Scanner(System.in);  int num1;  int num2;  int sum;    System.out.print("첫번째 숫자를 입력하시오: ");  num1 = input.nextInt();    System.out.print("두번째 숫자를 입력하시오: ");  num2 =input.nextInt();    sum = num1 + num2;    System.out.println(sum); 
 }}
