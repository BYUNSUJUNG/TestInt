public class ArrayLandomNumber {
 public static void main(String[] args) {
   int[] arr;
   arr=new int[30];
   for(int i=0; i<arr.length; i++)
     arr[i] = (int)(Math.random()*51);
   for(int i=0; i<arr.length; i++) {
     System.out.print(arr[i]+": ");
     for(int j=0;  j<arr[i]; j++)
       System.out.print("*");
     System.out.println(); 
   }
 }
}
