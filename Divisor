import java.util.*;public class Divisor {
 public static void main(String[] args) {  int input=0;  Scanner scan = new Scanner(System.in);    while(input<=0) {  System.out.print("수 입력: ");  input=scan.nextInt();  }      System.out.println(input+"의 약수는 다음과 같습니다");    for(int i=1; i<=input; i++) {   if(input%i==0) {    System.out.print(i+" ");   }  } }
}
