import java.util.*;public class Gcd3 {
 public static void main(String[] args) {  int num1=0, num2=1, rest;    Scanner scan = new Scanner(System.in);    while(num2>num1) {     System.out.print("두개의 정수를 입력하시오(큰수, 작은수):  ");  num1=scan.nextInt();  num2=scan.nextInt();  
  }    while(num2 != 0) {   rest=num1%num2;   num1=num2;   num2=rest;   }    System.out.print("최대 공약수는 "+num1); }
}
