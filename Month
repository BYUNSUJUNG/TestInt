import java.util.Scanner;public class Month {
 public static void main(String[] args) {
  int monthNumber;  String month;  Scanner scan = new Scanner(System.in);    System.out.print("달 입력: ");  month= scan.next();    switch(month) {  case  "january" :   monthNumber=1;    break;  case  "february":   monthNumber=2;   break;  case "march":   monthNumber=3;   break;  default:   monthNumber=0;   break;  }  System.out.println(monthNumber);   }}
