import java.util.*;
public class Araay {
  public static void main(String[] args) {
    int [][][] Student = new int [2][3][5];
    int point, sum=0;
    for (int i=0; i<2; i++ ) {
      for(int j=0; j<3; j++) {
        sum=0;
        for(int k=0; k<5; k++) {
          point = (int)(Math.random()*100);
          Student[i][j][k]=point;
          sum+=Student[i][j][k];
        }
        System.out.println("총점:"+sum+"평균:"+(sum/5));
      } 
    }
  }
}
