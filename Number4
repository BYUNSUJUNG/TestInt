import java.util.Scanner;public class Number4 {
 public static void main(String[] args) {  int integer1, integer2 ,integer3,  max;  Scanner input= new Scanner(System.in);  System.out.print("첫번째 정수: ");  integer1 = input.nextInt();  System.out.print("두번째 정수: ");  integer2 = input.nextInt();  System.out.print("세번째 정수:");  integer3 = input.nextInt();    if(integer1>integer2)    max = integer1;     else    max =integer2;    if(max>integer3)   max = integer3;    System.out.println("가장 큰 수는" + max);    }}
