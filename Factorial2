import java.util.*;public class Factorial2 {  public static void main(String[] args) { int input long fac=1;   Scanner scan = new Scanner(System.in);
   System.out.print("수 입력: ");   input=scan.nextInt();
   for(int i=1; i<=input; i++) {   fac=fac*i;    }    System.out.print("%d!dms %d입니다", input, fac);  }}
