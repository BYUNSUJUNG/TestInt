public class Dice1 {
 public static void main(String[] args) {  int count=0, num=0;  int Dice[] = {0,0,0,0,0,0};    while(count<12000) {   num=(int)(Math.random()*6);   Dice[num] ++;    count++;     }    System.out.println("=============");  System.out.println("\t"+" 면"+"\t"+"빈도");  System.out.println("=============");  for(int i=0; i<6; i++) {    System.out.println("\t"+(i+1)+"\t "+Dice[i]);  } }}
