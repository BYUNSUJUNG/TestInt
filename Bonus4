import java.util.Scanner;public class Bonus4 {
 public static void main(String[] args) {  final int goal, performance;  double  bonus;  Scanner scan = new Scanner(System.in);    System.out.print("목표을 입력하시오(단위: 만원): ");  goal=scan.nextInt();
  System.out.print("실적을 입력하시오(단위: 만원): ");  performance=scan.nextInt();    if(performance>goal) {   System.out.println("실적달성");   System.out.print("보너스");   bonus=(performance-goal)*0.1;   System.out.print((int)bonus);  }     }}
