import java.util.Scanner;public class ArrayTest4 {
 public static void main(String[] args) {  int[] scores = new int[5];  int sum=0;    Scanner scan = new Scanner(System.in);    for(int i=0; i<scores.length; i++) {   System.out.print("성적을 입력하시오: ");   scores[i]=scan.nextInt();   sum+=s[i];
  }    System.out.println("평균 성적은"+sum/5+"입니다");    
 }
}
