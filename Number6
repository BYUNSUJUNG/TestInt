import java.util.Scanner;public class Number6 {
 public static void main(String[] args) {
  int num=0;  Scanner scan = new Scanner(System.in);  
  System.out.print("정수를 입력하시오: ");  num=scan.nextInt();    switch(num) {    case 0:    System.out.println("ゼロ");   break;  case 1:   System.out.println("いち");   break;  case 2:   System.out.println("に");   break;  default:   System.out.println("おおく");   break;  }   }}
