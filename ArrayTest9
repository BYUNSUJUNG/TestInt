public class ArrayTest8 {       public static void main(String[] args) {        int[] randNumber = new int[5];        int  max=0, min=999;                for(int i=0; i<randNumber.length; i++) {         randNumber[i]=(int)(Math.random()*100+1);          System.out.print(randNumber[i]+" ");         System.out.println(" ");        }         for(int i=1; i<randNumber.length; i++) {         if(max<randNumber[i]) {          max=randNumber[i];         }         }         for(int i=0;i<randNumber.length;i++) {            if(randNumber[i]<min) {               min=randNumber[i];              }         }          System.out.print("최대값은"+max+", 최소값은"+min);       }}
