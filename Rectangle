import java.util.Scanner;public class Rectangle {
 public static void main(String[] args) {  Scanner input= new Scanner(System.in);  int width=0, height=0;  double area ,circumference;    System.out.print("사각형의 가로를 입력하시오: " );   width=input.nextInt();  System.out.print("사각형의 세로를 입력하시오: " );   height=input.nextInt();    area =width*height;  circumference=(width+height)*2;    System.out.println("사각형의 넓이는" + area   );   System.out.println("사각형의 둘레는" + circumference );  
 }}
