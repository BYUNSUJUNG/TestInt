import java.util.*;public class Sum3 {
 public static void main(String[] args) {  int num1, num2, sum=0;    Scanner scan = new Scanner(System.in);    System.out.print("첫번째 수 입력: ");  num1=scan.nextInt();  System.out.print("두번째 수 입력: ");  num2=scan.nextInt();
  for(int i=num1 ; i<=num2; i++)    sum+=i;    System.out.printf("%d부터 %d까지의 정수의 합= %d", num1, num2, sum);   }}
